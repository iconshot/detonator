{
  "name": "detonator",
  "version": "1.4.7",
  "description": "Build native mobile apps with Untrue.",
  "homepage": "https://detonator.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/iconshot/detonator.git"
  },
  "main": "dist/index.js",
  "bin": "cli/index.js",
  "scripts": {
    "prepare-template": "node scripts/prepare-template.js",
    "remove-dist": "rm -rf dist",
    "build-dist": "npm run remove-dist && tsc",
    "build": "npm run prepare-template && npm run build-dist",
    "publish": "npm run build && npm publish",
    "publish-alpha": "npm run build && npm publish --tag alpha"
  },
  "keywords": [
    "mobile",
    "app",
    "ui",
    "untrue"
  ],
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@paletto/color": "^1.1.4",
    "ts-loader": "^9.5.1",
    "webpack-cli": "^5.1.4",
    "fs-extra": "^11.2.0"
  },
  "peerDependencies": {
    "untrue": "^5.7.2"
  }
}
